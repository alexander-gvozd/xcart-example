model account_addresses {
  address_id             Int                     @id @default(autoincrement()) @db.UnsignedInt
  full_name              String?                 @db.VarChar(50)
  country                String                  @db.Char(2)
  phone_number           String                  @db.VarChar(50)
  street                 String                  @db.VarChar(50)
  detailed               String                  @db.VarChar(50)
  city                   String                  @db.VarChar(50)
  state                  Int                     @default(0)
  zip                    String                  @db.VarChar(50)
  is_default             Int?                    @db.TinyInt
  delivery_type_id       Int?                    @db.UnsignedInt
  user_id                Int                     @db.UnsignedInt
  address_type           String?                 @default("shipping") @db.Char(50)
  phone_ext              String?                 @db.VarChar(10)
  xcart_countries        xcart_countries         @relation(fields: [country], references: [code], onDelete: Cascade, map: "FK_account_addresses_xcart_countries")
  account_delivery_types account_delivery_types? @relation(fields: [delivery_type_id], references: [delivery_type_id], onDelete: Cascade, map: "FK_account_addresses_account_delivery_types")
  xcart_states           xcart_states            @relation(fields: [state], references: [stateid], onDelete: Cascade, map: "FK_account_addresses_xcart_states")
  xcart_users            xcart_users             @relation(fields: [user_id], references: [user_id], onDelete: Cascade, map: "FK_account_addresses_xcart_users")
  account_credit_cards   account_credit_cards[]

  @@index([delivery_type_id], map: "FK_account_addresses_account_delivery_types")
  @@index([country], map: "FK_account_addresses_xcart_countries")
  @@index([state], map: "FK_account_addresses_xcart_states")
  @@index([user_id], map: "FK_account_addresses_xcart_users")
}

model account_credit_cards {
  credit_card_id       Int                    @id @default(autoincrement()) @db.UnsignedInt
  name                 String?                @db.VarChar(50)
  address_id           Int                    @db.UnsignedInt
  is_default           Int?                   @default(0) @db.TinyInt
  card_number          String?                @db.VarChar(50)
  card_type            String?                @db.VarChar(50)
  expires              String?                @db.VarChar(250)
  user_id              Int                    @db.UnsignedInt
  account_addresses    account_addresses      @relation(fields: [address_id], references: [address_id], onDelete: Cascade, map: "FK_account_credit_cards_account_addresses")
  xcart_users          xcart_users            @relation(fields: [user_id], references: [user_id], onDelete: Cascade, map: "FK_account_credit_cards_xcart_users")
  account_transactions account_transactions[]

  @@index([address_id], map: "FK_account_credit_cards_account_addresses")
  @@index([user_id], map: "FK_account_credit_cards_xcart_users")
}

model account_decisions {
  decision_id       Int                 @id @default(autoincrement()) @db.UnsignedInt
  type              String              @db.VarChar(31)
