  order_id          Int
  order_number      String?             @db.VarChar(31)
  created           DateTime?           @default(now()) @db.DateTime(0)
  updated           DateTime?           @default(now()) @db.DateTime(0)
  xcart_orders      xcart_orders        @relation(fields: [order_id], references: [orderid], onDelete: Cascade, map: "account_decision_xcart_orders_orderid_fk")
  decision_licenses decision_licenses[]

  @@index([order_id], map: "account_decision_xcart_orders_orderid_fk")
}

model account_delivery_types {
  delivery_type_id  Int                 @id @default(autoincrement()) @db.UnsignedInt
