generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model a_coeff {
  manufacturerid     Int     @default(0)
  d_shiiping_a_coeff Decimal @default(0.00) @db.Decimal(12, 2)
  d_shipping_b_coeff Decimal @default(1.00) @db.Decimal(12, 2)

  @@ignore
